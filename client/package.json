{
  "name": "google-trends-remake",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/geo": "^0.61.1",
    "@nivo/line": "^0.61.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "fuzzy-search": "^3.2.1",
    "moment": "^2.24.0",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "npm-run-all start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css --env production",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "mv build ../client/dist/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/classnames": "^2.2.10",
    "@types/react-router-dom": "^5.1.3",
    "@types/fuzzy-search": "^2.1.0",
    "autoprefixer": "^9.7.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.7.5"
  }
